{
  "name": "milenmk/laravel-route-label",
  "description": "Add label support to Laravel routes with routeLabel() helper and Blade @routeLink directive",
  "license": "MIT",
  "authors": [
    {
      "name": "Milen Karaganski",
      "email": "milenmk@gmail.com"
    }
  ],
  "repository": [
    {
      "type": "git",
      "url": "https://github.com/milenmk/laravel-route-label"
    }
  ],
  "require": {
    "php": "^8.2|^8.3|^8.4",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/view": "^10.0|^11.0|^12.0",
    "illuminate/routing": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "laravel/pint": "^1.13",
    "squizlabs/php_codesniffer": "^3.11",
    "tightenco/duster": "^3.1",
    "phpunit/phpunit": "^12.1",
    "brianium/paratest": "^7.9",
    "orchestra/testbench": "^10.2"
  },
  "conflict": {
    "laravel/framework": "<10.0"
  },
  "autoload": {
    "psr-4": {
      "Milenmk\\LaravelRouteLabel\\": "src/"
    },
    "files": [
      "src/Helpers/enum_value.php",
      "src/Helpers/route_label.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Milenmk\\LaravelRouteLabel\\LaravelRouteLabelServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
